<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Prototypes.UI.ManagerMenuPage"
             Title="Manager Menu">
    <VerticalStackLayout>
        <Button
            Text="Songs"
            HorizontalOptions="Start"
            Clicked="OnSongs_Clicked"
            Background="White"
            TextColor="Black"
         />
        <Button
            Text="Not Checked In"
            HorizontalOptions="Start"
            Clicked="OnNotCheckedIn_Clicked"
            Background="White"
            TextColor="Black"
        />
        <Button
            Text="Checked In"
            HorizontalOptions="Start"
            Clicked="OnCheckedIn_Clicked"
            Background="White"
            TextColor="Black"
        />
        <Button
          Text="Access Codes"
          HorizontalOptions="Start"
          Clicked="OnAccessCodes_Clicked"
          Background="White"
          TextColor="Black"
        />
        <Button
          Text="Rehearsals"
          HorizontalOptions="Start"
          Clicked="OnRehearsals_Clicked"
          Background="White"
          TextColor="Black"
        />



    </VerticalStackLayout>
</ContentPage>